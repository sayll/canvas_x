{
  "name": "@sayll/canvas_x",
  "main": "./lib/canvas_x.js",
  "version": "0.0.3",
  "description": "canvas绘制工具库",
  "repository": {
    "type": "git",
    "url": "https://github.com/sayll/canvas_x"
  },
  "keywords": [
    "canvas_x",
    "canvas",
    "html",
    "js",
    "tool"
  ],
  "scripts": {
    "build": "rollup -c internals/prop.js",
    "build:dev": "rollup -c internals/dev.js -w",
